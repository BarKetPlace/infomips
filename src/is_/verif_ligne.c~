
#include <stdlib.h>
#include <stdio.h>
#include <string.h>
#include "is_.h"


int verif_ligne(char *ligne)
{
	char *r, *r2, *r3, *r4, *r5, *r6;
	r=strtok(ligne, ' ');
	r2=strtok(NULL, ' ');
	r3=strtok(NULL, ' ');
	r4=strtok(NULL, ' ');
	r5=strtok(NULL, ' ');
	r6=strtok(NULL, ' ');
	switch (r)
		case "load":
			if () {return 1;}
			else {return 0;}
		break;
		case "set":
			switch (r2)
				case "mem":
					if (is_type(r3)==0 && is_adresse(r4)==0 && isinteger(r5)==0) 
					{return 1;}
					else {return 0;}
				break;
				case "reg":
					if (is_registre(r3)==0 && isinteger(r4)==0) 
					{return 1;
					else {return 0;}
				break; 
						
							
		break;
		case "test": return 1;
		break;
		case "disp":
			switch (r2)
				case "mem":
					if (is_plage(r3)==0) {return 1;}
					else if (strcmp(r3,"map")==0) {return 1;}
					else {return 0;} 
				break;
				case "reg":
					if (is_registre(r3)==0) {return 1;}
					else {return 0;}
				break;
				default: {return 0;}
				break;
		break;
		case "resume": return 1;
		break;
		case "debug": return 1;
		break;
		case "exit": return 1;
		break;
		default: return 0;
			
}	

