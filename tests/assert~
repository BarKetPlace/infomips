load ./elf/boucle.o

#registres
#Erreur de syntaxe
assert v0 0
#registre inexistant
assert v3 0
#Correcte
assert $v0 0x0
assert $v1 0


#memoire
#Erreur de syntaxe
assert mem wor 0x5000 0xAAFFBBDD
assert mem word 5000 0xAAFFBBDD
assert mem word 0x5000 AAFFBBDD
#correcte
assert mem word 0x5000 0xAAFFBBDD
#correcte
assert mem byte 0x5000 0xDD
#case memoire trop elev√©e
assert mem word 0xffffff00 0xAAFFBBDD

exit
